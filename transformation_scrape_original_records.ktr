      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var temp = heading.replace("&lt;h1>","");
temp = temp.replace("&lt;/h1>","");

var month_expression = /january|february|march|april|may|june|july|august|september|october|november|december/i;
var year_expression = /1864|1865/ig;
var contact_expression = /headquarters|hdqrs|department|dept|circular|core|brigade|division|orders|special|continuation|road|office|chief|quartermaster|confederate|union|\-0-9|\-|\*/ig;
var location_expression = /augusta/i;
var location_generic_expression = /road/i;

var date;
var location;
var location_index;
var month_index;
var year_index;
var contact_from= "";

																					
if (content.match(year_expression) &amp;&amp; content.match(month_expression)){					//if content field contains month and year
	location_index = content.search(location_expression);
	month_index = content.search(month_expression);
	year_index = content.search(year_expression);

	if (content.match(location_expression)){											//identify location names similar to month names
		
		var temp_location_index = content.lastIndexOf(",");								//extract location field
		location = content.substring(0, content.lastIndexOf(",", temp_location_index-1));

		var temp_content = content.substring(location.length, content.length);
		month_index = temp_content.search(month_expression);
		year_index = temp_content.search(year_expression);
		date = temp_content.substring(month_index, year_index+4);						//extract date field after location field

	}
	else{
		date = content.substring(month_index, year_index+4);							//extract date field

		if (content.substring(0, month_index-1) == content.substring(0, 0)){			//if string before date is null
			if (date == date.toUpperCase()){
				location = "";
			}
			else{
				//do nothing
			}
		}
		else{
			location = content.substring(0, month_index-1);
			rtrim(location);
	
			if (location.length>50){													//exclude location fields larger than 50 characters
				location = "";
				// do nothing
			}
			else if (location.substring(location.length-1, location.length) == ","){  	//if comma exists in location field, remove it
				location = location.substring(0, location.length-1);
			}
			else {
				//do nothing
			}
		}
		contact_from = "";
	}
}
else{
	if (content == content.toUpperCase()){												//if content field is uppercase
		if (content.match(location_generic_expression)){								//if content field matches generic location
			location = trim(content.replace(",",""));									//remove commas, trim content field, save as location
			contact_from = "";
			//date = "";
		}
		else if (content.match(contact_expression)){									//ignore generic uppercase names		
			contact_from = "";
			location = "";
			date = "";
		}
		else{
			contact_from = trim(content.replace(",",""));								//remove commas, trim content field, save as contact_from
		}
	}
	else{
		contact_from = "";
	}
}</jsScript_script>
